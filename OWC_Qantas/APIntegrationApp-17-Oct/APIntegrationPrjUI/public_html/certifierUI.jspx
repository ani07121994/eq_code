      <af:resource type="javascript">
        function keyPressInputText(evt) {
            var source = evt.getSource();
            var keyCodePressed = evt.getKeyCode();
            if (keyCodePressed == AdfKeyStroke.TAB_KEY) {
                var custEvent = new AdfCustomEvent(source, "onKeyPressTextField", 
                {
                    submittedValue : source.getSubmittedValue(), localValue : source.getValue()
                },
true);
                custEvent.queue();
            }
        }
      </af:resource>
