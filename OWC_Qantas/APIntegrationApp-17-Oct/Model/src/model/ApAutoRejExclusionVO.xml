<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ApAutoRejExclusionVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.ApAutoRejExclusionVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindExclusionString"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select count(1) as Flag from (
SELECT exclusion_string
    FROM ap_auto_rej_exclusion
    WHERE EXCLUSION_TYPE        = 'EMAIL'
    AND upper(exclusion_string) = upper(:bindExclusionString)
    UNION
    SELECT exclusion_string
    FROM ap_auto_rej_exclusion
    WHERE EXCLUSION_TYPE = 'PATTERN'
    AND upper(:bindExclusionString) LIKE upper(exclusion_string))]]>
  </SQLQuery>
  <ViewAttribute
    Name="Flag"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="String"
    AliasName="FLAG"
    Expression="FLAG"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
