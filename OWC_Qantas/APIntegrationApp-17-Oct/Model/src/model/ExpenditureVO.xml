<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ExpenditureVO"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  ComponentClass="model.ExpenditureVOImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.ExpenditureVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindExpenditure"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select hou.name expenditure_types
  from apps.hr_organization_units hou, 
       apps.hr_organization_information hoi
 where hoi.org_information1 = 'PA_EXPENDITURE_ORG' -- with classification 'Project Expenditure/Event Organization'
   and hou.organization_id = hoi.organization_id
   and (hou.date_to is null or hou.date_to > trunc(sysdate))]]>
  </SQLQuery>
  <ViewAttribute
    Name="ExpenditureTypes"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EXPENDITURE_TYPES"
    Expression="EXPENDITURE_TYPES"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewCriteria
    Name="ExpenditureVOCriteria"
    ViewObjectName="model.ExpenditureVO"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow25"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="ExpenditureVOCriteria_vcrow25_ExpenditureTypes"
        ViewAttribute="ExpenditureTypes"
        Operator="STARTSWITH"
        Conjunction="AND"
        Value=":bindExpenditure"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
