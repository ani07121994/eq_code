  <SQLQuery>
    <![CDATA[SELECT hou.NAME operating_unit,
       aps.segment1 vendor_number,
       aps.vendor_id,
       aps.vendor_name,
       assa.org_id,
       assa.vendor_site_id,
       assa.vendor_site_code,
       assa.vat_registration_num tax_registration_num_supp_site,
       assa.invoice_currency_code currency_code,
       assa.vat_code tax_rate_code,
       atr.NAME payment_terms,
       assa.address_line1 || ' ' || assa.address_line2 || ' ' ||
       assa.address_line3 || ' ' || assa.city || ' ' || assa.state || ' ' ||
       assa.zip || ' ' || assa.country address
  FROM apps.ap_suppliers          aps,
       apps.ap_supplier_sites_all assa,
       apps.hr_operating_units    hou,
       apps.ap_terms atr
 WHERE aps.vendor_id = assa.vendor_id
   AND assa.org_id = hou.organization_id
   AND aps.vendor_type_lookup_code NOT IN
       ('CUSTOMER CARE', 'WORKERS COMPENSATION', 'EMPLOYEE')
   AND atr.term_id(+) = assa.terms_id]]>
  </SQLQuery>
