            //getDBTransaction().rollback();
            e.printStackTrace();
        }finally{
            if(rowSetIterator!=null){
                rowSetIterator.closeRowSetIterator();    
            }
        }
        System.out.println("End: modifyLineItemDetails()" + new Date());
    }
    
