  <SQLQuery>
    <![CDATA[SELECT DISTINCT 
       hou.NAME expenditure_org
      ,hou.date_from start_date
      ,hopu.NAME operating_unit_name
      ,hopu.organization_id org_id
  FROM apps.hr_organization_units       hou
      ,apps.hr_organization_information hoi
      ,apps.fnd_flex_values             ffv2
      ,apps.fnd_flex_values_tl          ffvt2
      ,apps.fnd_flex_value_sets         ffvs2
      ,apps.hr_operating_units          hopu
 WHERE hoi.org_information1 = 'PA_EXPENDITURE_ORG'
   AND hou.organization_id = hoi.organization_id
   AND ffv2.flex_value_set_id = ffvs2.flex_value_set_id
   AND ffv2.flex_value_id = ffvt2.flex_value_id
   AND ffvs2.flex_value_set_name = 'QFEQGL_Centre'
   AND hou.NAME LIKE ffv2.flex_value || '%'
   AND hopu.NAME = ffv2.attribute6
   AND (hou.date_to IS NULL OR hou.date_to > trunc(SYSDATE))]]>
  </SQLQuery>
