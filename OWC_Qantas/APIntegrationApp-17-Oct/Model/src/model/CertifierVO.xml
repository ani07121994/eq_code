<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CertifierVO"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  ComponentClass="model.CertifierVOImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Coll|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.CertifierVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select papf.employee_number,
          papf.global_name certifier_name,
          papf.email_address 
     from apps.per_All_people_f papf
    where trunc(sysdate) between trunc(papf.effective_start_date) and trunc(papf.effective_end_date)]]>
  </SQLQuery>
  <ViewAttribute
    Name="EmployeeNumber"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
