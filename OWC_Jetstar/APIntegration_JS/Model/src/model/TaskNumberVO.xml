  pt.task_name,
  pt.task_id,
  pt.project_id ,
  ppa.segment1
FROM apps.pa_tasks pt,
  apps.pa_projects_All ppa
WHERE pt.project_id    = ppa.project_id
AND pt.chargeable_flag = 'Y'
and pt.billable_flag = 'Y'
AND pt.task_id NOT    IN
  (SELECT a.task_id
  FROM apps.pa_tasks a,
    apps.pa_tasks b
  WHERE a.project_id = ppa.project_id
  AND A.task_id      = b.parent_task_id
