    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT hou.name operating_unit
,pha.segment1 po_num
,pha.type_lookup_code po_type
,pha.creation_date
,pha.closed_code po_status
,decode(pha.cancel_flag,NULL,'NO','N','NO','YES') PO_CANCELLED_STATUS
,aps.segment1 vendor_number
,aps.vendor_name
,assa.vendor_site_id
,assa.vendor_site_code
FROM apps.ap_suppliers aps
,apps.ap_supplier_sites_all assa
,apps.hr_all_organization_units hou
,apps.po_headers_all pha
,apps.fnd_lookup_values flv
WHERE aps.vendor_id = assa.vendor_id
AND assa.org_id = hou.organization_id
AND aps.vendor_type_lookup_code NOT IN
('CUSTOMER CARE'
,'WORKERS COMPENSATION'
,'EMPLOYEE')
AND pha.authorization_status NOT IN
('INCOMPLETE'
,'REQUIRES REAPPROVAL')
AND pha.type_lookup_code NOT IN ('CONTRACT','BLANKET')
AND pha.vendor_site_id = assa.vendor_site_id]]>
  </SQLQuery>
