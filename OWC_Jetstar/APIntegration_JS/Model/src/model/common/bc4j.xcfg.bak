<?xml version = '1.0' encoding = 'UTF-8'?>
<BC4JConfig version="11.1" xmlns="http://xmlns.oracle.com/bc4j/configuration">
   <AppModuleConfigBag ApplicationName="model.APOperationAM">
      <AppModuleConfig name="APOperationAMShared" ApplicationName="model.APOperationAM" DeployPlatform="LOCAL" jbo.project="model.Model" java.naming.factory.initial="oracle.jbo.common.JboInitialContextFactory">
         <AM-Pooling jbo.ampool.isuseexclusive="false" jbo.ampool.maxpoolsize="1"/>
         <Security AppModuleJndiName="model.APOperationAM"/>
         <Custom JDBCDataSource="java:comp/env/jdbc/eqfiles_connDS"/>
      </AppModuleConfig>
      <AppModuleConfig DeployPlatform="LOCAL" jbo.project="model.Model" name="APOperationAMLocal" ApplicationName="model.APOperationAM">
         <Database jbo.TypeMapEntries="Java" jbo.locking.mode="optimistic" jbo.server.internal_connection="jdbc/PROD_EQFILES_NXA"/>
         <Security AppModuleJndiName="model.APOperationAM"/>
         <Custom JDBCDataSource="jdbc/EQFILES_NXA"/>
      </AppModuleConfig>
   </AppModuleConfigBag>
   <AppModuleConfigBag ApplicationName="model.EbsAM">
      <AppModuleConfig DeployPlatform="LOCAL" jbo.project="model.Model" name="EbsAMLocal" ApplicationName="model.EbsAM">
         <AM-Pooling jbo.ampool.timetolive="120000" jbo.ampool.maxinactiveage="60000" jbo.ampool.monitorsleepinterval="30000"/>
         <Database jbo.locking.mode="optimistic" jbo.server.internal_connection="jdbc/PROD_EQFILES_NXA"/>
         <Security AppModuleJndiName="model.EbsAM"/>
         <Custom JDBCDataSource="jdbc/PROD_APPS_NXA"/>
      </AppModuleConfig>
      <AppModuleConfig name="EbsAMShared" ApplicationName="model.EbsAM" DeployPlatform="LOCAL" jbo.project="model.Model" java.naming.factory.initial="oracle.jbo.common.JboInitialContextFactory">
         <AM-Pooling jbo.ampool.isuseexclusive="false" jbo.ampool.maxpoolsize="1"/>
         <Security AppModuleJndiName="model.EbsAM"/>
         <Custom JDBCDataSource="java:comp/env/jdbc/eqfiles_connDS"/>
      </AppModuleConfig>
   </AppModuleConfigBag>
</BC4JConfig>
