<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="WhtCodeVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.WhtCodeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindOrgId"
    Kind="viewcriteria"
    Type="java.math.BigDecimal"/>
  <Variable
    Name="bindWhtCode"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select awt.tax_name wht_code, 
       awt.org_id
  from apps.ap_awt_group_taxes_all awt
 where exists
 (select 1
          from apps.ap_tax_codes_all      atc,
               apps.ap_awt_tax_rates_all  atr,
               apps.gl_code_combinations  gcc,
               apps.ap_supplier_sites_All assa,
               apps.ap_suppliers          aps
         WHERE atc.NAME = ATR.tax_name
           and atc.org_id = ATR.ORG_ID
           and atc.tax_code_combination_id = gcc.code_combination_id
           and gcc.enabled_flag = 'Y'
           and atc.awt_vendor_site_id = assa.vendor_site_id
           and aps.vendor_id = assa.vendor_id
           and nvl(aps.end_date_active, SYSDATE) >= SYSDATE
           and nvl(aps.enabled_flag, 'X') = 'Y'
           and nvl(assa.inactive_date, SYSDATE) >= SYSDATE
           and nvl(atc.inactive_date, SYSDATE) >= SYSDATE
           and trunc(SYSDATE) BETWEEN nvl(atr.start_date, trunc(SYSDATE)) AND
               nvl(atr.end_date, trunc(SYSDATE))
           and awt.org_id = atr.org_id
           and awt.tax_name = atr.tax_name)
 order by awt.org_id, awt.tax_name]]>
  </SQLQuery>
  <ViewAttribute
    Name="WhtCode"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="25"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WHT_CODE"
    Expression="WHT_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="BIGINT"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="16"/>
    </DesignTime>
  </ViewAttribute>
  <ViewCriteria
    Name="WhtCodeVOCriteria"
    ViewObjectName="model.WhtCodeVO"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow73"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="WhtCodeVOCriteria_vcrow73_WhtCode"
        ViewAttribute="WhtCode"
        Operator="CONTAINS"
        Conjunction="AND"
        Value=":bindWhtCode"
        IsBindVarValue="true"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="WhtCodeVOCriteria_vcrow73_OrgId"
        ViewAttribute="OrgId"
        Operator="="
        Conjunction="AND"
        Value=":bindOrgId"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
