<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TaxRateCodeLookupVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.TaxRateCodeLookupVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bind_poNumber"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <Variable
    Name="bind_siteId"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <Variable
    Name="bind_taxRateCode"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT tax_rate_code, tax_rate, segment1 AS ponum, vendor_site_id
  FROM (SELECT zl.tax_rate_code,
               zl.tax_rate,
               pha.segment1 segment1,
               NULL vendor_site_id
          FROM apps.po_distributions_all  pod,
               apps.po_line_locations_all pll,
               apps.po_lines_all          pol,
               apps.zx_lines              zl,
               apps.zx_rates_b            zrb,
               apps.po_headers_all        pha
         WHERE pha.po_header_id = pol.po_header_id
           AND pol.po_line_id = pll.po_line_id
           AND pll.line_location_id = pod.line_location_id
           AND zl.trx_id = pol.po_header_id
           AND zl.trx_line_id = pll.line_location_id
           AND zrb.tax_rate_id = zl.tax_rate_id
           AND nvl(pha.attribute_category, 'XXXXXX') <> 'TRAX'
        UNION ALL
        SELECT DISTINCT zb.tax_rate_code,
                        zb.percentage_rate,
                        pha.segment1,
                        NULL
          FROM zx_rates_b            zb,
               zx_rates_tl           zt,
               zx_id_tcc_mapping_all zov,
               apps.po_headers_all   pha
         WHERE zb.tax_rate_id = zt.tax_rate_id
           AND zov.tax_rate_code_id = zb.tax_rate_id
           AND pha.org_id = zov.org_id
           AND zb.active_flag = 'Y'
           AND rate_type_code <> 'RECOVERY'
           AND pha.attribute_category = 'TRAX'
           AND trunc(SYSDATE) BETWEEN zb.effective_from AND
               nvl(zb.effective_to, trunc(SYSDATE) + 1)
           AND decode(record_type_code,
                      'MIGRATED',
                      'INPUT',
                      'USER_DEFINED',
                      zov.tax_type) = nvl(zb.tax_class, 'GST')
        UNION ALL
        SELECT DISTINCT zb.tax_rate_code,
                        zb.percentage_rate,
                        NULL segment1,
                        assa.vendor_site_id
          FROM zx_rates_b                 zb,
               zx_rates_tl                zt,
               zx_id_tcc_mapping_all      zov,
               apps.ap_supplier_sites_all assa
         WHERE zb.tax_rate_id = zt.tax_rate_id
           AND zov.tax_rate_code_id = zb.tax_rate_id
           AND assa.org_id = zov.org_id
           AND zb.active_flag = 'Y'
           AND rate_type_code <> 'RECOVERY'
           AND trunc(SYSDATE) BETWEEN zb.effective_from AND
               nvl(zb.effective_to, trunc(SYSDATE) + 1)
           AND decode(record_type_code,
                      'MIGRATED',
                      'INPUT',
                      'USER_DEFINED',
                      zov.tax_type) = nvl(zb.tax_class, 'GST'))]]>
  </SQLQuery>
  <ViewAttribute
