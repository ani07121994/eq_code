      <ViewCriteriaItem
        Name="TaxRateCodeLookupVOCriteria_vcrow51_Ponum"
        ViewAttribute="Ponum"
        Operator="="
        Conjunction="AND"
        Value=":bind_poNumber"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ViewCriteria
    Name="TaxRateCodeLookupVOCriteria_lookupBySiteId"
    ViewObjectName="model.TaxRateCodeLookupVO"
