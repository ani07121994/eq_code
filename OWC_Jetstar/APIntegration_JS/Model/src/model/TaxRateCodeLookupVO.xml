    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="TAX_RATE_CODE"
    Expression="TAX_RATE_CODE"
    SQLType="VARCHAR">
