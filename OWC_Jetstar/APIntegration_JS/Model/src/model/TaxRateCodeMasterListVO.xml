<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TaxRateCodeMasterListVO"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.TaxRateCodeMasterListVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select tax_rate_code, percentage_rate from 
(SELECT DISTINCT zb.tax_rate_code
               ,zb.percentage_rate
               ,zov.org_id
  FROM zx_rates_b            zb
      ,zx_rates_tl           zt
      ,zx_id_tcc_mapping_all zov
-- ,apps.ap_supplier_sites_all assa
 WHERE zb.tax_rate_id = zt.tax_rate_id
   AND zov.tax_rate_code_id = zb.tax_rate_id
      -- AND assa.org_id = zov.org_id            
   AND zb.active_flag = 'Y'
   AND rate_type_code <> 'RECOVERY'
   AND trunc(SYSDATE) BETWEEN zb.effective_from AND
       nvl(zb.effective_to
          ,trunc(SYSDATE) + 1)
   AND decode(record_type_code
             ,'MIGRATED'
             ,'INPUT'
             ,'USER_DEFINED'
             ,zov.tax_type) = nvl(zb.tax_class
                                 ,'GST')
) group by tax_rate_code,percentage_rate]]>
  </SQLQuery>
  <ViewAttribute
    Name="TaxRateCode"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="TAX_RATE_CODE"
    Expression="TAX_RATE_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PercentageRate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="PERCENTAGE_RATE"
    Expression="PERCENTAGE_RATE"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
