<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TaxRateCodeVO"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.TaxRateCodeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bind_poNum"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bind_siteId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindTaxRateCode"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[Select  zl.tax_rate_code,zl.tax_rate
  FROM   apps.po_distributions_all  pod
            ,apps.po_line_locations_all pll
            ,apps.po_lines_all          pol
            ,apps.zx_lines              zl 
            ,apps.zx_rates_b            zrb 
            ,apps.po_headers_all       pha
      WHERE  pha.segment1 = :bind_poNum
      and    pha.po_header_id = pol.po_header_id
      AND    pol.po_line_id = pll.po_line_id
      AND    pll.line_location_id = pod.line_location_id
      AND    zl.trx_id = pol.po_header_id 
      AND    zl.trx_line_id = pll.line_location_id 
      AND    zrb.tax_rate_id = zl.tax_rate_id

union --all

select supp.vat_code,zx.percentage_rate
  from apps.ap_supplier_sites_all supp, apps.zx_rates_b zx
 where supp.vendor_site_id = :bind_siteId
 and zx.tax_rate_code = supp.vat_code]]>
  </SQLQuery>
  <ViewAttribute
    Name="TaxRateCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="TAX_RATE_CODE"
    Expression="TAX_RATE_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TaxRate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="TAX_RATE"
    Expression="TAX_RATE"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewCriteria
    Name="TaxRateCodeVOCriteria"
    ViewObjectName="model.TaxRateCodeVO"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow33"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="TaxRateCode"
        ViewAttribute="TaxRateCode"
        Operator="STARTSWITH"
        Conjunction="AND"
        Value=":bindTaxRateCode"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
