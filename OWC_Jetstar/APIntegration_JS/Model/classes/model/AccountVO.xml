  <SQLQuery>
    <![CDATA[SELECT ffv.flex_value ACCOUNT, ffv.description account_description 
                  FROM apps.fnd_flex_value_sets ffvs, apps.fnd_flex_values_vl ffv 
                 WHERE ffvs.flex_value_set_name = 'QFEQGL_Account' 
                   AND ffvs.flex_value_set_id = ffv.flex_value_set_id 
                   AND ffv.enabled_flag = 'Y' 
                   AND SYSDATE BETWEEN NVL(start_date_active, SYSDATE) AND 
                           NVL(end_date_active, SYSDATE + 1) 
                   AND NOT EXISTS 
                 (SELECT 'NOT EXISTS' 
                                  FROM apps.fnd_flex_value_sets ffvs1, apps.fnd_flex_values_vl ffv1 
                                 WHERE ffvs1.flex_value_set_name = 'QFEQPA_WIP_ACCOUNTS' 
                                   AND ffvs1.flex_value_set_id = ffv1.flex_value_set_id 
                                   AND ffv1.enabled_flag = 'Y' 
                                   AND SYSDATE BETWEEN NVL(start_date_active, SYSDATE) AND 
                                           NVL(end_date_active, SYSDATE + 1) 
                                   AND ffv.flex_value = ffv1.flex_value)]]>
  </SQLQuery>
