  <SQLQuery><![CDATA[SELECT *   
  FROM ((SELECT ppf.first_name,ppf.middle_names,ppf.last_name,ppf.employee_number,ppf.email_address,hru.name department,ppf.person_id,   
        ppf.first_name || ' ' || ppf.last_name as conName,decode(ppf.current_employee_flag,'N','No',Null,'No','Yes') current_employee_flag, 
        'Qantas' as operating_name,'QF' as jetstar_group,'QF' AS org   
        FROM apps.per_all_people_f          ppf,apps.per_all_assignments_f     paf,apps.hr_all_organization_units hru,   
        apps.per_person_types          ppt,apps.per_person_type_usages_f  pptu,apps.per_periods_of_service    pos   
        WHERE TRUNC(sysdate) between TRUNC(ppf.effective_start_date) AND TRUNC(ppf.effective_end_date)   
        AND ppf.person_id = paf.person_id AND ppf.current_employee_flag = 'Y' AND paf.primary_flag = 'Y' AND TRUNC(sysdate) between TRUNC(paf.effective_start_date) AND   
        TRUNC(paf.effective_end_date) AND paf.organization_id = hru.organization_id AND ppt.system_person_type IN ('EMP') AND ppf.person_id = pptu.person_id   
        AND pptu.person_type_id = ppt.person_type_id AND ppf.effective_start_date between trunc(pptu.effective_start_date) AND trunc(pptu.effective_end_date)   
        AND pos.person_id(+) = paf.person_id AND pos.period_of_service_id(+) = paf.period_of_service_id AND (pos.actual_termination_date > trunc(sysdate) OR   
        pos.actual_termination_date IS NULL) -- 41872  34940   
        AND hru.name not in (select hau.name from apps.hr_all_organization_units hau where upper(name) like upper('%Jetstar%') or upper(name) like upper('%JS%')))   
		union all  
        (select first_name,middle_names,last_name,employee_number,email_address,department,person_id,conname,current_employee_flag,operating_unit,jetstar_group,'JS' AS org   
        from apps.JSEQ_CERTIFIER_REASSGN_LOV_MV))]]></SQLQuery>
