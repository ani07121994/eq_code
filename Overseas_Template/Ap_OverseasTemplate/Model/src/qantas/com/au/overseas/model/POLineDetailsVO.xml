  <SQLQuery>
    <![CDATA[SELECT tab.po_number
      ,tab.line_num
      ,nvl(SUM(CASE
                 WHEN tab.matching_basis = 'QUANTITY' THEN
                  tab.quantity
                 ELSE
                  NULL
               END)
          ,1) quantity
      ,
       -- tab.line_quantity,
       tab.uom
      ,nvl(SUM(CASE
                 WHEN tab.matching_basis = 'AMOUNT' THEN
                  tab.unit_price
                 ELSE
                  NULL
               END)
          ,tab.unit_price) unit_price
      ,tab.vendor_id
      ,tab.vendor_site_id
      ,tab.org_id
      ,tab.item_description
  FROM (SELECT pha.segment1 po_number
              ,pha.vendor_id
              ,pha.vendor_site_id
              ,pha.org_id
              ,pla.line_num
              ,pla.quantity line_quantity
              ,pla.amount
              ,pla.matching_basis
              ,pla.item_description
              ,pha.po_header_id
              ,CASE
                 WHEN pla.matching_basis = 'AMOUNT' THEN
                  1
                 WHEN pla.matching_basis = 'QUANTITY' THEN
                  (CASE
                 WHEN (plla.quantity - plla.quantity_billed) < 0 THEN
                  0
                 ELSE
                  (plla.quantity - plla.quantity_billed)
               END) END quantity
              ,CASE
                 WHEN pla.matching_basis = 'QUANTITY' THEN
                  pla.unit_price
                 WHEN pla.matching_basis = 'AMOUNT' THEN
                  (CASE
                 WHEN (plla.amount - plla.amount_billed) < 0 THEN
                  0
                 ELSE
                  (plla.amount - plla.amount_billed)
               END) END unit_price
              ,pla.unit_meas_lookup_code uom
              ,(SELECT NAME
                  FROM apps.hr_organization_units
                 WHERE organization_id = pla.org_id) operating_unit
          FROM apps.po_headers_all        pha
              ,apps.po_lines_all          pla
              ,apps.po_line_locations_all plla
         WHERE pha.po_header_id = pla.po_header_id
           AND plla.po_header_id = pha.po_header_id
           AND plla.po_line_id = pla.po_line_id
           AND pha.authorization_status NOT IN
               ('INCOMPLETE', 'REQUIRES REAPPROVAL')
        --AND PHA.ATTRIBUTE_CATEGORY = 'AVIATION FUEL'
        ) tab
 WHERE 1 = 1
 GROUP BY tab.po_number
         ,tab.line_num
         ,tab.uom
         ,tab.line_quantity
         ,unit_price
         ,org_id
         ,tab.vendor_id
         ,tab.vendor_site_id
         ,tab.item_description]]>
  </SQLQuery>
