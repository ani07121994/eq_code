  <SQLQuery><![CDATA[((SELECT hou.name operating_unit  
,pha.segment1 po_num  
,pha.type_lookup_code po_type  
,pha.po_header_id  
,pha.creation_date  
,pha.closed_code po_status  
,decode(pha.cancel_flag,NULL,'NO','N','NO','YES') PO_CANCELLED_STATUS  
,aps.segment1 vendor_number  
,aps.vendor_name  
,assa.vendor_site_id  
,assa.vendor_site_code 
,nvl(assa.vat_registration_num,aps.vat_registration_num) VatRegistrationNum 
,'QF' AS org 
FROM apps.ap_suppliers aps  
,apps.ap_supplier_sites_all assa  
,apps.hr_all_organization_units hou  
,apps.po_headers_all pha 
WHERE aps.vendor_id = assa.vendor_id  
AND assa.org_id = hou.organization_id  
AND aps.vendor_type_lookup_code NOT IN  
('CUSTOMER CARE'  
,'EMPLOYEE')  
AND pha.authorization_status NOT IN  
('INCOMPLETE'  
,'REQUIRES REAPPROVAL')  
AND pha.type_lookup_code NOT IN ('CONTRACT','BLANKET')  
AND nvl(pha.attribute_category  
                 ,'XXXXX') NOT IN ('AVIATION FUEL', 'EINVOICE')  
AND pha.type_lookup_code = 'STANDARD'  
AND pha.vendor_site_id = assa.vendor_site_id 
AND hou.name  in 
               (select hau.name 
                  from apps.hr_all_organization_units hau 
                 where upper(name) like upper('%Qantas%') 
                    or upper(name) like upper('%QF%'))) 
union all 
(SELECT hou.name operating_unit 
,pha.segment1 po_num 
,pha.type_lookup_code po_type 
,pha.po_header_id 
,pha.creation_date 
,pha.closed_code po_status 
,decode(pha.cancel_flag,NULL,'NO','N','NO','YES') PO_CANCELLED_STATUS 
,aps.segment1 vendor_number 
,aps.vendor_name 
,assa.vendor_site_id 
,assa.vendor_site_code 
,'' AS VatRegistrationNum 
,'JS' AS org 
FROM apps.ap_suppliers aps 
,apps.ap_supplier_sites_all assa 
,apps.hr_all_organization_units hou 
,apps.po_headers_all pha 
WHERE aps.vendor_id = assa.vendor_id 
AND assa.org_id = hou.organization_id 
AND aps.vendor_type_lookup_code NOT IN 
('CUSTOMER CARE' 
,'EMPLOYEE') 
AND pha.authorization_status NOT IN 
('INCOMPLETE' 
,'REQUIRES REAPPROVAL') 
AND pha.type_lookup_code NOT IN ('CONTRACT','BLANKET') 
AND pha.vendor_site_id = assa.vendor_site_id 
AND hou.name  in 
               (select hau.name 
                  from apps.hr_all_organization_units hau 
                 where upper(name) like upper('%Jetstar%') 
                    or upper(name) like upper('%JS%'))))]]></SQLQuery>
