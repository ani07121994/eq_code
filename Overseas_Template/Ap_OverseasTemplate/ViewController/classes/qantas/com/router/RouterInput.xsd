<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.InvoiceRouter.org" targetNamespace="http://www.InvoiceRouter.org"
     elementFormDefault="qualified">
    <xsd:element name="invoiceImport">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="header" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="documentType" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="invoiceType" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="orgID" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="invoiceNumber" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="invoiceDate" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="vendorId" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="vendorName" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="vendorNumber" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="vendorSiteID" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="InvoiceSiteID" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="PONumber" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="tax" type="xsd:float" minOccurs="0"/>
                            <xsd:element name="withholdingTax" type="xsd:float" minOccurs="0"/>
                            <xsd:element name="invoiceTotalAmount" type="xsd:float" minOccurs="0"/>
                            <xsd:element name="currencyCode" type="xsd:string" minOccurs="0"/>
