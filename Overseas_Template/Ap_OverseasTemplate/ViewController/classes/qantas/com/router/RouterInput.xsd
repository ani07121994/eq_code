                            <xsd:element name="Elect_Remittance" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="additionalinfo" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="docID" type="xsd:string" minOccurs="0"/>
