                                        <xsd:element name="Line5" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Line6" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Line7" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Line8" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Line9" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Line10" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
