                            <xsd:element name="ABN" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Subtotal" type="xsd:float" minOccurs="0"/>
                            <xsd:element name="accountNumber" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Freight" type="xsd:decimal" minOccurs="0"/>
                            <xsd:element name="misc" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="currencyEquivalent" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="WHTAmount" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Inv_Filename" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Certifier_number" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Certifier_email" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Certification_Flag" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Payment_group" type="xsd:string" minOccurs="0"/>
