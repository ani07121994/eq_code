                                        <xsd:element name="TaxRate" type="xsd:float" minOccurs="0"/>
                                        <xsd:element name="GLCode" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="ProjectCode" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="TaskNo" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="ExpDate" type="xsd:string" minOccurs="0"/>
